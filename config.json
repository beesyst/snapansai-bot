            "ubuntu": {
                "unset": "unset GTK_PATH",
                "install": "sudo apt update && sudo apt install -y gnome-screenshot",
                "check": "gnome-screenshot --version",
                "run": "/usr/bin/gnome-screenshot -f"
            },
            "windows": {
                "install": "pip install pyautogui",
                "run": "pyautogui.screenshot('screenshot.png')"
            },
            "macos": {
                "install": "brew install screenresolution",
                "run": "screencapture -x ~/Pictures/screenshot.png"
            },
            "fedora": {
                "install": "sudo dnf install -y gnome-screenshot",
                "run": "/usr/bin/gnome-screenshot -f"
            }
        }
    }
